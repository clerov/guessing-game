use rand::Rng;
use std::io;
use std::Camp::Oder

fn main() {
    println!("Welcome to Guessing Game!");
    println!("Enter the guessed number: ");

    let mut guess = String::new();
    io::stdin()
        .read_line(&mut guess)
        .expect("Unable to readline");

    println!("You guessed {guess}");
}
